<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
</head>

<body>
    <h1>
        <%= family.name %> page
    </h1>
    <P>
        <%= family.description %>
    </P>

    <!-- list of genesus* -->
    <ul>
        <% genus_list.forEach(genus=> { %>
            <li><a href="<%= genus.url %>">
                    <%= genus.name %>
                </a></li>
            <% }); %>
    </ul>
</body>

</html>